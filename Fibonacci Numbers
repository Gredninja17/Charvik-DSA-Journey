import java.util.Scanner; 

public class Fibonacci{
    public static void main(String [] args){
        Scanner in = new Scanner(System.in);
        System.out.println("Please enter the number of fibonacci numbers you desire");
        int n = in.nextInt();
        int one = 0;
        int two = 1;

        for (int i = 0; i < n; i++) {
            System.out.print(" " + one);
            int next = one + two;
            one = two;
            two = next;
        }
    }
}
